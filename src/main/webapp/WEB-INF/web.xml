<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>sgp</display-name>


	<!-- ListerCollaborateursController -->
	<servlet>
		<servlet-name>ListerCollaborateursController</servlet-name>
		<servlet-class>dev.sgp.web.ListerCollaborateursController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ListerCollaborateursController</servlet-name>
		<url-pattern>/collaborateurs/lister</url-pattern>
	</servlet-mapping>

	<!-- EditerCollaborateurController -->
	<servlet>
		<servlet-name>EditerCollaborateurController</servlet-name>
		<servlet-class>dev.sgp.web.EditerCollaborateurController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>EditerCollaborateurController</servlet-name>
		<url-pattern>/collaborateurs/editer</url-pattern>
	</servlet-mapping>


	<!-- Creation CollaborateurController -->

	<servlet>
		<servlet-name>CreationCollaborateurController</servlet-name>
		<servlet-class>dev.sgp.web.CreationCollaborateurController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>CreationCollaborateurController</servlet-name>
		<url-pattern>/collaborateurs/creation</url-pattern>
	</servlet-mapping>

</web-app>